<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.qwkj.qwkjaccountcenter.modules.test.dao.TestTest1Dao">

    <sql id="selectSql">
        SELECT
        T.ID,
        T.NAME,
        T.AGE,
        T.CREATE_TIME,
        T.UPDATE_TIME
        FROM TEST_TEST1 T
        WHERE 1=1
        <if test="params.name != null and params.name.trim() != ''">
            AND T.NAME LIKE '%${params.name}%'
        </if>
    </sql>

    <select id="selectTestTest1Page" resultType="com.qwkj.qwkjaccountcenter.modules.test.entity.TestTest1Entity">
        <include refid="selectSql"/>
    </select>

    <select id="queryAll" resultType="com.qwkj.qwkjaccountcenter.modules.test.entity.TestTest1Entity">
        <include refid="selectSql"/>
    </select>
</mapper>
